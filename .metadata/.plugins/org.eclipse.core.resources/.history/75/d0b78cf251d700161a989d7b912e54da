import java.util.ArrayList;

public class Ex08ArrayList03 {
public static void main(String[] args) {
		
		try{
			
			// . Crear un tercer ArrayList amb tots els seus valors de tipus String, amb 8 valors inicials de diferents paraules.
			
			ArrayList<String> firstarray = new ArrayList<String>();
			
			firstarray.add("Hola");
			firstarray.add("esto");
			firstarray.add("es");
			firstarray.add("un");
			firstarray.add("array");
			firstarray.add("de");
			firstarray.add("ocho");
			firstarray.add("strings");
			
			
			for(int i = 0;i<firstarray.size();i++){
				 System.out.println(firstarray.get(i));
				}
		        
		}
		
		catch (Exception e){
			System.out.println("Error al introducir los números " + e);
		}
		
	}
	
}
