
public class ex07 {
	public static void main(String[] args){

		int [][] matrix = new int[5][5];
	
		
		matrix[0][0] = 1;
		matrix[0][1] = 0; 
		matrix[0][2] = 0;
		matrix[0][3] = 0;
		matrix[0][4] = 0;
		
		matrix[1][0] = 0;
		matrix[1][1] = 2; 
		matrix[1][2] = 0;
		matrix[1][3] = 0;
		matrix[1][4] = 0;
		
		matrix[2][0] = 0;
		matrix[2][1] = 0; 
		matrix[2][2] = 3;
		matrix[2][3] = 0;
		matrix[2][4] = 0;
		
		matrix[3][0] = 0;
		matrix[3][1] = 0; 
		matrix[3][2] = 0;
		matrix[3][3] = 4;
		matrix[3][4] = 0;
		
		matrix[4][0] = 0;
		matrix[4][1] = 0; 
		matrix[4][2] = 0;
		matrix[4][3] = 0;
		matrix[4][4] = 5;
		
		int aux=0;
		
		for(int i = 0, j= matrix.length-1 ; i<(matrix.length/2) ; i++, j--){
			aux = matrix[i][i];
			matrix[i][i]=matrix[j][j];
			matrix[j][j]=aux;
		}
		
		for (int i = 0; i < matrix.length; i++) {
			for (int j = 0; j < matrix[i].length; j++) {
				System.out.print (matrix[i][j]);
			}  
			System.out.println ("");
		}
		
	}
}